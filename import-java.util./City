//kyle
public class City
{
   private Unit unit;
   private Sea port;
   private boolean rebellion;
   private boolean famine;
   private Province province;
   public City()
   {
      province = null;
      port = null;
      unit = null;
      famine = false;
      rebellion = false;
   }
   public City(Province p)
   {
      province = p;
      port = null;
      unit = null;
      famine = false;
      rebellion = false;
   }
   public City(Province p, Sea harbor)
   {
      province = p;
      port = harbor;
      unit = null;
      famine = false;
      rebellion = false;
   }
}
